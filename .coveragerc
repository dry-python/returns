[run]
omit =
    returns/contrib/mypy/*
    returns/contrib/pytest/*.py
    returns/contrib/hypothesis/*
    *test*

[report]
fail_under = 90
exclude_lines =
    pragma: no cover
    def _is_anyio_available
    def _is_trio_available
    def _is_in_trio_context
    def detect_async_context
    if not has_anyio
    if not has_trio
    return has_anyio
    return has_trio
    if context == "trio" and has_anyio
    except RuntimeError:
    except Exception:
    def __init__
    def __aenter__
    def __aexit__
    ->exit
